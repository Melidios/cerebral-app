<h4 class="mt-3">Welcome</h4>
<% if logged_in? %>
  <h1>You are Logged In, <%= current_user.email %></h1>
<%end%>
<div class="d-flex justify-content-center">
  <%= button_to "Log In", '/login', method: :get, class: 'purple-btn m-2'%>
  <%= button_to "Sign Up", '/users/new', method: :get, class: 'black-btn m-2'%>
</div>